<?xml version="1.0" encoding="UTF-8"?>
<CompositeView
    width="{config_control_bar.width}"
    layout="{layout.control_bar}">
  
  <ImageView
      id="home_img"
      width="{config_control_bar.width}"
      height="{config_control_bar.item_height}"
      src="{img(images/icon/controlbar_setting_near_normal.png)}"
      multiState="{config_control_bar.home_img_MultiState}"
      scaleType="{enum.ImageView.ScaleType.Centerinside}" />
  <TextView
      id="home_tv"
      text="home"
      color="{config_control_bar.title_color}"
      fontSize="{config_control_bar.title_fontSize}" 
      align="{enum.TextView.Align.Center}"
      verticalAlign="{enum.TextView.VerticalAlign.Middle}"/>

  <ImageView
      id="list_img"
      width="{config_control_bar.width}"
      height="{config_control_bar.item_height}" 
      src="{img(images/icon/controlbar_setting_near_normal.png)}"
      multiState="{config_control_bar.list_img_MultiState}"
      scaleType="{enum.ImageView.ScaleType.Centerinside}" />
  <TextView
      id="list_tv"
      text="list"
      color="{config_control_bar.title_color}"
      fontSize="{config_control_bar.title_fontSize}" 
      align="{enum.TextView.Align.Center}"
      verticalAlign="{enum.TextView.VerticalAlign.Middle}"/>
  

  <ImageView
      id="navi_img"
      width="{config_control_bar.width}"
      height="{config_control_bar.item_height}"
      src="{img(images/icon/controlbar_setting_near_normal.png)}"
      multiState="{config_control_bar.navi_img_MultiState}"
      scaleType="{enum.ImageView.ScaleType.Centerinside}" />
  <TextView
      id="navi_tv"
      text="navi"
      color="{config_control_bar.title_color}"
      fontSize="{config_control_bar.title_fontSize}" 
      align="{enum.TextView.Align.Center}"
      verticalAlign="{enum.TextView.VerticalAlign.Middle}"/>

  <ImageView
      id="setting_img"
      width="{config_control_bar.width}"
      height="{config_control_bar.item_height}"
      src="{img(images/icon/controlbar_setting_near_normal.png)}"
      multiState="{config_control_bar.setting_img_MultiState}"
      scaleType="{enum.ImageView.ScaleType.Centerinside}" />
  <TextView
      id="setting_tv"
      text="setting"
      color="{config_control_bar.title_color}"
      fontSize="{config_control_bar.title_fontSize}" 
      align="{enum.TextView.Align.Center}"
      verticalAlign="{enum.TextView.VerticalAlign.Middle}"/>

</CompositeView>