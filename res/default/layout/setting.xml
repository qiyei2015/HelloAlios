<?xml version="1.0" encoding="UTF-8"?>
<CompositeView xsi:noNamespaceSchemaLocation="CLOUD_APP_MARKUP"
    layout="{layout.setting}" propertySetName="setting">

  <include
      markup="common_title_bar"
      id="action_bar" />

    <CompositeView 
        id="setting_content" 
        background="#FF6600"
        layout="{layout.setting_content}">

        <CheckBox
            id="checkBox1"
            text="设置1"
            width="{dp(160)}"
            height="{dp(160)}"
            multiState="{config.common_button_multistate}"
        />

        <Switch
            id="switch1"
            text="设置2"
            width="{dp(200)}"
            height="{dp(100)}"
            buttonColor="pink"
            value="false"
            multiState="{config.common_button_multistate}"
        />
    </CompositeView>

</CompositeView>





