{
    "home": {
        "type": "RelativeLayout",
        "params": {
            "title": {
                "align": {
                    "middle": "parent",
                    "center": "parent"
                },
                "margin": {
                    "left": "{config.common_left_margin}"
                }
            },
            "home_btn": {
                "align": {
                    "top": "parent",
                    "left": "parent"
                },
                "margin": {
                    "top": "{config.common_left_margin}",
                    "left": "{config.common_left_margin}"
                }
            },
            "main_btn": {
                "align": {
                    "top": {
                        "target": "home_btn",
                        "side": "bottom"
                    },
                    "left": {
                        "target": "home_btn",
                        "side": "left"
                    },
                    "right": {
                        "target": "home_btn",
                        "side": "right"
                    }
                },
                "margin": {
                    "top": "{config.common_left_margin}"
                }
            },
            "navi_btn": {
                "align": {
                    "top": {
                        "target": "main_btn",
                        "side": "bottom"
                    },
                    "left": {
                        "target": "main_btn",
                        "side": "left"
                    },
                    "right": {
                        "target": "main_btn",
                        "side": "right"
                    }
                },
                "margin": {
                    "top": "{config.common_left_margin}"
                }
            },
            "setting_btn": {
                "align": {
                    "top": {
                        "target": "navi_btn",
                        "side": "bottom"
                    },
                    "left": {
                        "target": "navi_btn",
                        "side": "left"
                    },
                    "right": {
                        "target": "navi_btn",
                        "side": "right"
                    }
                },
                "margin": {
                    "top": "{config.common_left_margin}"
                }
            }
        }
    },
    "main": {
        "type": "RelativeLayout",
        "params": {
            "title": {
                "align": {
                    "middle": "parent",
                    "center": "parent"
                }
            }
        }
    }
}