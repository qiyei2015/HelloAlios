{
    "item_list_with_scroll": {
        "type": "RelativeLayout",
        "params": {
            "0": {
                "align": {
                    "left": "parent",
                    "right": "parent",
                    "top": "parent",
                    "bottom": "parent"
                },
                "margin": {
                    "left": "{config.margin_item_list_content}",
                    "right": "{config.margin_item_list_content}"
                }
            }
        }
    },
    "item_list": {
        "type": "RelativeLayout",
        "params": {
            "thumb_img": {
                "align": {
                    "left": "parent",
                    "middle": "parent"
                }
            },
            "title": {
                "align": {
                    "left": {
                        "target": 0,
                        "side": "right"
                    },
                    "top": {
                        "target": 0,
                        "side": "top"
                    }
                },
                "margin": {
                    "left": "{config.common_left_margin}"
                }
            }
        }
    }
}